{
  "name": "@playkit-js/playkit-js-blinking-watermark",
  "version": "1.0.0",
  "main": "dist/playkit-blinking-watermark.js",
  "types": "types.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "npm run prepare:demo -- dev && npm run build:dev && webpack serve --open --mode development",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "watch": "webpack --progress --colors --watch --mode development",
    "lint:check": "eslint src --ext .ts,.tsx --color",
    "lint:fix": "npm run lint:check -- --fix",
    "prettier:fix": "prettier --write .",
    "prepare:demo": "sh scripts/prepare-demo.sh",
    "docs:generate": "typedoc",
    "types:generate": "tsc src/index.ts types/global.d.ts --declaration --emitDeclarationOnly --outFile types/modules.ts --jsx react --jsxFactory h --stripInternal true --target ESNext --moduleResolution node",
    "release": "standard-version",
    "pushTaggedRelease": "git push --follow-tags --no-verify origin main",
    "commit:dist": "git add --force --all dist demo/index.html && (git commit -m 'chore: update dist' || exit 0)"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master",
    "lint-staged": "^10.5.3",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.2.8",
    "typedoc": "^0.22.17",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "peerDependencies": {
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "watermark",
    "kaltura",
    "player",
    "playkit-js",
    "playkit-js-blinking-watermark",
    "html5 player"
  ],
